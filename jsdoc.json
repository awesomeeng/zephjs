{
    "env": {
        "conf": {
            "plugins": [],
            "recurseDepth": 10,
            "source": {
                "includePattern": ".+\\.js(doc|x)?$",
                "excludePattern": "(^|\\/|\\\\)_"
            },
            "sourceType": "module",
            "tags": {
                "allowUnknownTags": true,
                "dictionaries": [
                    "jsdoc",
                    "closure"
                ]
            },
            "templates": {
                "monospaceLinks": true,
                "cleverLinks": true,
                "default": {
                    "outputSourceFiles": true
                }
            },
			"opts": {
				"encoding": "utf8",
				"recurse": true
			}
        }
    }
}
